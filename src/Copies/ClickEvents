const Home = () => {

    const handleClick=()=>{
        console.log("Hello ")
    }
    const handleClickAgain=(name,e)=>{
        console.log('Hollo' + name,e.target);
    }
    return ( 
        <div className="home">
            <h2>Homepage</h2>
            <button onClick={handleClick}>Click me</button>
            <button onClick={(e)=>handleClickAgain(" Soumya",e)}>Click meAgain</button>
        
        
        
        </div>
     );
}
 
export default Home;